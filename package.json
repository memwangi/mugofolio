    "express": "^4.17.2",
    "framer-motion": "4",
    "next": "12.0.4",
    "next-sanity": "^0.4.0",
    "node-fetch": "^3.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-portable-text": "^0.4.0",
    "ts-node": "^10.4.0"
  },
